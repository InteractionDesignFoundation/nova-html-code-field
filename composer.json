{
    "name": "interaction-design-foundation/nova-html-code-field",
    "description": "An HTML Laravel Nova field.",
    "keywords": [
        "laravel",
        "nova"
    ],
    "license": "MIT",
    "require": {
        "php": ">=8.0",
        "laravel/nova": "^3.0"
    },
    "require-dev": {
        "mockery/mockery": "^1.4",
        "orchestra/testbench": "^6.0",
        "phpunit/phpunit": "^9.4"
    },
    "repositories": [
        {
            "type": "composer", "url": "https://nova.laravel.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "InteractionDesignFoundation\\NovaHtmlCodeField\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "InteractionDesignFoundation\\NovaHtmlCodeField\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "InteractionDesignFoundation\\NovaHtmlCodeField\\ServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "test": "phpunit"
    }
}
